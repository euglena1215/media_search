<h1>Imageの登録</h1>

<form method="post" action="/create">
  <div class="form-group">
    <label>タイトル</label>
    <input type="text" class="form-control" name="title" size="15"/>
  </div>
  <div class="form-group">
  <label>画家</label>
    <input type="text" class="form-control" name="author" size="15"/>
  </div>
  <div class="form-group">
    <label>画像URL</label>
    <input type="text" class="form-control" name="url" size="15"/>
  </div>
  
  <input type="submit" class="btn btn-primary" value="登録"/>
</form>
